# Backend Configuration
NODE_ENV=development
PORT=3001

# Logging
LOG_LEVEL=info

# Cache Configuration  
CACHE_TTL_TICKERS=10000
CACHE_TTL_FUNDING_RATES=600000
CACHE_TTL_PROCESSED_DATA=10000

# API Configuration
API_RATE_LIMIT_WINDOW=60000
API_RATE_LIMIT_MAX=100

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000,http://localhost:4000,http://localhost:3002,http://frontend:3000

# Hyperliquid Configuration (Optional)
# HYPERLIQUID_WALLET_ADDRESS=your_wallet_address_here
# HYPERLIQUID_PRIVATE_KEY=your_private_key_here

# Health Check Configuration
HEALTH_CHECK_TIMEOUT=5000
HEALTH_CHECK_INTERVAL=30000

# Development Configuration
DEV_ENABLE_DEBUG_LOGS=true
DEV_ENABLE_CACHE_LOGS=true
